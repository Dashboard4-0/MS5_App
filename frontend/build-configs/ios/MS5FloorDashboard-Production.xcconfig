// MS5.0 Floor Dashboard - iOS Production Build Configuration

// Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = com.ms5floordashboard

// App Display Name
PRODUCT_NAME = MS5.0 Floor Dashboard

// Version Information
MARKETING_VERSION = 1.0.0
CURRENT_PROJECT_VERSION = 1

// Deployment Target
IPHONEOS_DEPLOYMENT_TARGET = 11.0
TARGETED_DEVICE_FAMILY = 1,2

// Build Configuration
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) PRODUCTION=1
GCC_WARN_64_TO_32_BIT_CONVERSION = YES
GCC_WARN_ABOUT_RETURN_TYPE = YES
GCC_WARN_UNDECLARED_SELECTOR = YES
GCC_WARN_UNINITIALIZED_AUTOS = YES
GCC_WARN_UNUSED_FUNCTION = YES
GCC_WARN_UNUSED_VARIABLE = YES

// Code Signing
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = 
CODE_SIGN_IDENTITY = iPhone Distribution

// Build Settings
ENABLE_BITCODE = NO
SWIFT_OPTIMIZATION_LEVEL = -O
SWIFT_VERSION = 5.0

// React Native Configuration
RCT_NEW_ARCH_ENABLED = NO
USE_HERMES = YES

// API Configuration (Production)
API_BASE_URL = https://api.ms5dashboard.com
WS_BASE_URL = wss://api.ms5dashboard.com

// Feature Flags
FEATURE_REAL_TIME_UPDATES = 1
FEATURE_ADVANCED_ANALYTICS = 1
FEATURE_ESCALATION_SYSTEM = 1
FEATURE_OFFLINE_SUPPORT = 1

// Debug Configuration
DEBUG_MODE = 0
